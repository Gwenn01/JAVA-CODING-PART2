import java.util.*;
public class Main {
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		
		System.out.print("Enter a number: ");
		int n = s.nextInt();
		
		String result = "";
		
		if(n% 3 == 0 && n%5 == 0){
		    result = "FizzBuzz";
		}else if(n% 3 == 0){
		    result = "Fizz";
		}else if(n% 5 == 0){
		    result = "Buzz";
		}else {
		    result = Integer.toString(n);
		}
		System.out.print(result);
	}
}