public class Main {
	public static void main(String[] args) {
		
		int[] arr = {3, 2, 7, 4, 5};
		
		for(int i = 0; i <= arr.length-1; i++){
		    System.out.print(arr[i] + " ");
		}
		
		int max = 0;
		
		for(int i = 0; i <= arr.length-1; i++){
		   if(arr[i]  >= max){
		       max = arr[i];
		   }	   
		}
		System.out.println();
		System.out.print("Remove Max: ");
		for(int i = 0; i <= arr.length-1; i++){
		   if(arr[i] != max){
		      System.out.print(arr[i] + " ");
		   }
		}
		
	}
}