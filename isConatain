public class Main {
	public static void main(String[] args) {
		boolean isContain = false;
		
		int[] arr = {5, 7};
		
	    for(int i = 0; i < arr.length; i++){
	        if(arr[i] == 4 || arr[i] == 7){
	            isContain = true;
	            break;
	        }
	    }
	    System.out.print(isContain);
	
	}
}