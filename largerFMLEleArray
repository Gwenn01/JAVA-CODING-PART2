public class Main {
	public static void main(String[] args) {
		int[] arr = {20, 100, 80 , 50, 67};
		int max = arr[0];
		
		for(int i = 0; i < arr.length; i++){
		    if(i != 1 && i != 3){
		        if(arr[i] > max){
		            max = arr[i];
		        }
		    }
		}
		System.out.print(max);
		
	}
}